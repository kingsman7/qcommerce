        "route": "qform.admin.fields.index",
        "tooltip":"isite.cms.label.view",
        "label": {
          "isTranslatable": "singular",
          "value": "isite.cms.form.actions"
        },
        "action": {
          "type":"exteralUrl - ref - method - helper - modal",
          "useStore": true,
          "domine":"qsiteApp.baseUrl",
          "value":"item.slug", 
          "method": "showAuctionData",
          "externalUrl": "/anuncios/${item.slug}",
          "params":[
            {
              "name":"auctionId",
              "param":"id",
              "modalProps":{
                "title": "iauctions.cms.newBid"
              }
            }
          ]
        },
        "format": {
          "type": "str - conditional",
          "validationAppMode": {
            "mode":"iadmin"
          },
          "value":1,
          "child":"status",
          "yes":{
            "vIf": false
          }
           ,
          "no": {
            "vIf": false
          }
        }
      }
    ]
